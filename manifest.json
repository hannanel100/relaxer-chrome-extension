{
  "manifest_version": 2,

  "name": "Relaxer",
  "description": "Helps you to remember to breath and relax",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "browser_action": {
    "default_icon": "./img/bg.jpg"
  },

  "permissions": ["tabs"],

  "background": ["background.js"]
}
